<!DOCTYPE html>
<html>
<head>
	<title>Planilha <%= title %></title>
</head>
<body>
	<table width="300px" border="1px solid black">
		<tr>
			<th>Gateway ID</th>
			<th>E-MAIL</th>
			<th>Temperatura</th>
		</tr>
		<% for(var i = 0; i < rows.length; i++){ %>
			<tr>
				<td style="text-align:center"><%= rows[i].gateway %></td>
				<td style="text-align:center"><%= rows[i].email %></td>
				<td style="text-align:center"><%= rows[i].temperatura_controle %></td>
			</tr>
		<% } %>
	</table>
</body>
</html>
